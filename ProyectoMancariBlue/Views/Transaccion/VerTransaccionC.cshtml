@{
    ViewData["Title"] = "VerTransaccionC";
}

<div class="app-title">
    <div>
        <h1><i class="fa fa-th-list" style="margin-right: 10px;"></i>Detalle de Transaccion</h1>
        <p>Transacciones</p>

    </div>
    <ul class="app-breadcrumb breadcrumb side">
        <li class="breadcrumb-item"><i class="fa fa-home fa-lg"></i></li>
        <li class="breadcrumb-item">Transacciones</li>
    </ul>
</div>
<div class="col-md-12">
    <div class="tile">
        <h3 class="tile-title">Ver Detalle de Transacción</h3>
        <div class="tile-body">
            <form asp-action="Index" asp-controller="Transaccion" method="post" enctype="multipart/form-data">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label">Codígo</label>
                            <input  class="form-control" value="001-C" readonly />
                        </div>
                        <div class="form-group">
                            <label class="control-label">Proveedor</label>
                            <input  class="form-control" value="Proveedor 1" readonly />
                        </div>
                        <div class="form-group">
                            <label class="control-label">Identificación</label>
                            <input  class="form-control" value="3-2123-2123" readonly />
                        </div>
                        <div class="form-group">
                            <label class="control-label">Categoría</label>
                            <input class="form-control" value="Categoría 1" readonly />
                
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label class="control-label">Producto</label>
                            <input class="form-control" value="Producto 1" readonly />
                        </div>
                        <div class="form-group">
                            <label class="control-label">Descripcion</label>
                            <input class="form-control" value="Compra de Producto 1" readonly/>

                        </div>
                        <div class="form-group">
                            <label class="control-label">Importe Compra</label>
                            <input class="form-control" value="₡120.000" readonly />

                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <a class="btn btn-secondary" href="javascript:history.back();"><i class="fa fa-fw fa-lg fa-times-circle"></i>Regresar</a>
                </div>
            </form>
        </div>
    </div>
</div>