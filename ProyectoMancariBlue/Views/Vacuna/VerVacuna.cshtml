@{
    ViewData["Title"] = "VerVacuna";
}

<div class="app-title">
    <div>
        <h1><i class="fa fa-th-list" style="margin-right: 10px;"></i>Detalle de Vacunas</h1>
        <p>Vacunas</p>

    </div>
    <ul class="app-breadcrumb breadcrumb side">
        <li class="breadcrumb-item"><i class="fa fa-home fa-lg"></i></li>
        <li class="breadcrumb-item">Vacunas</li>
    </ul>
</div>
<div class="col-md-12">
    <div class="tile">
        <h3 class="tile-title">Detalle de Vacunas </h3>
        <div class="tile-body">
            <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label class="control-label">Código Animal</label>
                    <input class="form-control" value="001" readonly />
                </div>
                <div class="form-group">
                    <label class="control-label">Codigo Vacuna</label>
                    <input class="form-control" value="1234" readonly />

                </div>
                <div class="form-group">
                    <label class="control-label">Fecha Aplicación</label>
                    <input class="form-control" value="15/01/2024" readonly />

                </div>
                <div class="form-group">
                    <label class="control-label">Vacuna</label>
                    <input class="form-control" value="Vacuna 1" readonly />

                </div>
                <div class="form-group">
                    <label class="control-label">Categoría Vacuna</label>
                    <input class="form-control" value="Desparacitante" readonly />
                </div>
                <div class="form-group">
                    <label class="control-label">Próxima Aplicación</label>
                    <input class="form-control" value="25/01/2024" readonly />
                </div>
            </div>
        </div>

                <div class="form-group">
                   <a class="btn btn-secondary" href="javascript:history.back();"><i class="fa fa-fw fa-lg fa-times-circle"></i>Regresar</a>
                </div>
        </div>
    </div>
</div>